# Parameters file
# Project : From geodynamic to Seismic observations in the Earth's inner core


method:

   ## seismic 
   raypath: True # True for averaging over raypath (False to calculate only at turning point)
   Nlines: 20 #number of points (3184 for results.dat)

   # if real data set	
   # s_type: real data
   # fname: results.dat #if real data
   # columns: [9, 10, 11, 12, 13, 14, 15] #if real data
   # lines: 1 #if real data
   # 

   ## if random data set
   s_type: random
   random_type: surface # if random. can be "surface" or "surface_equatorial" (latitude = 0.) 
   s_dmin: 0. #if random. minimum depth of turning point in km. 
   s_dmax: 100. #if random. maximum depth or turning point in km.

   ## geodynamics
   g_type: translation
   v: 3. # units is Ric/tau_ic 

Nd: 20 #number of points in the trajectory (for average and plot)
Ric: 1221. #radius IC